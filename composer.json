{
  "name": "lone/sensible",
  "description": "a theme with vite, vuejs and tailwind",
  "license": "MIT",
  "authors": [
    {
      "name": "IngoGutwin",
      "email": "onmabu@gmail.com"
    }
  ],
  "require": {
    "vinkla/headache": "^3.5",
    "enshrined/svg-sanitize": "^0.22.0",
    "wpackagist-plugin/advanced-custom-fields": "^6.6.2",
    "wpackagist-plugin/wordpress-seo": "^26.3"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "extra": {
    "installer-paths": {
      "../../plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "../../mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ]
    }
  }
}
